{
  "name": "eslint-config-airbe",
  "version": "1.0.0",
  "description": "A shared eslint config to apply eslint fastly",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "watch": "tsup --format esm,cjs --watch",
    "lint": "eslint . --ext .ts,.vue,.tsx,.d.ts",
    "lint:fix": "eslint . --fix --ext .ts,.vue,.tsx,.d.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "code style",
    "lint",
    "code lint",
    "code format"
  ],
  "author": "Akino",
  "license": "MIT",
  "peerDependencies": {
    "@eslint/js": "^9.6.0",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.27.0",
    "typescript": "*",
    "typescript-eslint": "8.0.0-alpha.41",
    "vue-eslint-parser": "^9.4.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vue": "^3.4.31"
  },
  "dependencies": {
    "@eslint/js": "^9.6.0",
    "deepmerge": "^4.3.1",
    "eslint-plugin-vue": "^9.27.0",
    "typescript-eslint": "8.0.0-alpha.41",
    "vue-eslint-parser": "^9.4.3"
  }
}
